<mavlink>
	<include>common.xml</include>

	<enums>
	<enum name="LISUM_SENSOR_AIRSPEED_FLAGS" bitmask="true">
		<description>Airspeed sensor flags</description>
		<entry value="0" name="LISUM_SENSOR_AIRSPEED_UNHEALTHY">
			<description>Airspeed sensor is unhealthy</description>
		</entry>
		<entry value="1" name="LISUM_SENSOR_AIRSPEED_USING">
			<description>True if the data from this sensor is being actively used by the flight controller for guidance, navigation or control.</description>
		</entry>
	</enum>

	<enum name="LISUM_GNSS_RECV">
		<description>These values determine the source of GNSS data.</description>
		<entry value="0" name="LISUM_GNSS_RECV_INTERNAL">
			<description>Internal GNSS.</description>
		</entry>
		<entry value="1" name="LISUM_GNSS_RECV_EXTERNAL">
			<description>External GNSS.</description>
		</entry>
	</enum>

	<enum name="LISUM_GNSS_RECV_FIX_TYPE">
		<description>These values determine GNSS fix type.</description>
		<entry value="0" name="LISUM_GNSS_RECV_FIX_TYPE_NO_FIX">
			<description>Fix is Invalid.</description>
		</entry>
		<entry value="1" name="LISUM_GNSS_RECV_FIX_TYPE_DEAD_RECKON_ONLY">
			<description>Fix is based on Dead Reckoning.</description>
		</entry>
		<entry value="2" name="LISUM_GNSS_RECV_FIX_TYPE_2D">
			<description>Fix is calculated using 3 satellites.</description>
		</entry>
		<entry value="3" name="LISUM_GNSS_RECV_FIX_TYPE_3D">
			<description>Fix is calculated using at least 4 satellites.</description>
		</entry>
		<entry value="4" name="LISUM_GNSS_RECV_FIX_TYPE_DEAD_RECKON">
			<description>Fix is calculated using GNSS data and Dead Reckoning.</description>
		</entry>
		<entry value="5" name="LISUM_GNSS_RECV_FIX_TYPE_TIME">
			<description>Time Fix</description>
		</entry>
	</enum>

	<enum name="LISUM_GNSS_RECV_CARR_SOL">
		<description>These values determine GNSS carrier phase range solution.</description>
		<entry value="0" name="LISUM_GNSS_RECV_CARR_SOL_NONE">
			<description>No carrier phase range solution.</description>
		</entry>
		<entry value="1" name="LISUM_GNSS_RECV_CARR_SOL_FLOAT">
			<description>Carrier phase range solution with floating ambiguities.</description>
		</entry>
		<entry value="2" name="LISUM_GNSS_RECV_CARR_SOL_FIXED">
			<description>Carrier phase range solution with fixed ambiguities.</description>
		</entry>
	</enum>

	<enum name="LISUM_HORNET_HOMING_METHOD">
		<description>These values determine Hornet X01 homing method.</description>
		<entry value="0" name="LISUM_HORNET_HOMING_METHOD_ACT_POS">
			<description>Homing using actual position.</description>
		</entry>
		<entry value="1" name="LISUM_HORNET_HOMING_METHOD_CURR_THR">
			<description>Homing using current threshold.</description>
		</entry>
	</enum>

	<enum name="LISUM_HORNET_MODE">
		<description>These values determine Hornet X01 Mode.</description>
		<entry value="0" name="LISUM_HORNET_MODE_OPERATIONAL">
			<description>Operational Mode.</description>
		</entry>
		<entry value="1" name="LISUM_HORNET_MODE_MAINTENANCE">
			<description>Maintenance Mode.</description>
		</entry>
	</enum>

	<enum name="MAV_CMD">
		<!-- Manual control/Hornet messages (59328 - 59335) -->
		<entry value="59328" name="MAV_CMD_HORNET_HOMING" hasLocation="false" isDestination="false">
			<description>Perform Hornet X01 Homing.</description>
			<param index="1" label="Method" enum="LISUM_HORNET_HOMING_METHOD">Method.</param>
			<param index="2">Empty.</param>
			<param index="3">Empty.</param>
			<param index="4">Empty.</param>
			<param index="5">Empty.</param>
			<param index="6">Empty.</param>
			<param index="7">Empty.</param>
		</entry>

		<entry value="59329" name="MAV_CMD_HORNET_ARM_DISARM" hasLocation="false" isDestination="false">
			<description>Arm/Disarm Hornet X01.</description>
			<param index="1" label="Arm" minValue="0" maxValue="1" increment="1">0: disarm, 1: arm</param>
			<param index="2">Empty.</param>
			<param index="3">Empty.</param>
			<param index="4">Empty.</param>
			<param index="5">Empty.</param>
			<param index="6">Empty.</param>
			<param index="7">Empty.</param>
		</entry>

		<entry value="59330" name="MAV_CMD_HORNET_INIT" hasLocation="false" isDestination="false">
			<description>Initialize Hornet X01.</description>
			<param index="1">Empty.</param>
			<param index="2">Empty.</param>
			<param index="3">Empty.</param>
			<param index="4">Empty.</param>
			<param index="5">Empty.</param>
			<param index="6">Empty.</param>
			<param index="7">Empty.</param>
		</entry>

		<entry value="59331" name="MAV_CMD_HORNET_FAULT_RESET" hasLocation="false" isDestination="false">
			<description>Reset Hornet X01 Faults.</description>
			<param index="1">Empty.</param>
			<param index="2">Empty.</param>
			<param index="3">Empty.</param>
			<param index="4">Empty.</param>
			<param index="5">Empty.</param>
			<param index="6">Empty.</param>
			<param index="7">Empty.</param>
		</entry>

		<entry value="59332" name="MAV_CMD_HORNET_SWEEP" hasLocation="false" isDestination="false">
			<description>Execute Hornet X01 actuator sweep.</description>
			<param index="1">Empty.</param>
			<param index="2">Empty.</param>
			<param index="3">Empty.</param>
			<param index="4">Empty.</param>
			<param index="5">Empty.</param>
			<param index="6">Empty.</param>
			<param index="7">Empty.</param>
		</entry>
	</enum>

	</enums>

