		<field type="uint16_t" name="turn_on">Turn ON Struct</field>
		<field type="uint8_t" name="valve_state">Valve State</field>
		<field type="uint8_t" name="pressure_state">Pressure State</field>
		<field type="uint32_t" name="warning">Warning Struct</field>
